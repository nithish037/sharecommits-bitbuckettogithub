{
    "printWidth": 120,
    "tabWidth": 4,
    "singleQuote": true,
    "arrowParens": "always",
    "trailingComma": "es5",
    "overrides": [
        {
            "files": ["*.json", ".*rc"],
            "options": {
                "trailingComma": "none"
            }
        }
    ],
    "endOfLine": "auto",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "semi": true
}
